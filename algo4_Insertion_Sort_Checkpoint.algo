PROCEDURE swap(VAR xp, VAR yp : INTEGER)
VAR
    tmp : INTEGER;
BEGIN
    tmp := xp;
    xp := yp;
    yp := tmp;
END

ALGORITHM insertionSort
VAR
    arr : ARRAY OF INTEGER [10];
    i, j : INTEGER;
BEGIN
    FOR i FROM 1 TO arr.length-1 STEP 1 DO
        j = i;
        WHILE (j > 0 && arr[j-1] > arr[j]) THEN
            swap(arr[j-1], arr[j]);
            j = j-1;
        END_WHILE;
    END_FOR;
END